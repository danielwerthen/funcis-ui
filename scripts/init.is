let view = '<button id="foo">Hello world</button>'
let view2 = '<textarea id="foo" placeholder="jade lang"></textarea><div id="bar"></div>'

UI.OnConnection()
	(id) =>
		UI.Html('#main', view2, id)
			UI.Bind('#foo', 'change keyup', {}, id)
